{
  "name": "@kronos-integration/svelte-components",
  "version": "0.0.0-semantic-release",
  "main": "src/index.mjs",
  "svelte": "src/index.svelte",
  "description": "Svelte components for Kronos",
  "keywords": [
    "component",
    "svelte",
    "web"
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "start": "rollup -c example/rollup.config.mjs -w",
    "test": "testcafe $BROWSER:headless tests/*-test.js -s test-results --app-init-delay 500 --app \"rollup -c example/rollup.config.mjs -w\"",
    "docs": "documentation readme src/index.mjs --section=API",
    "lint": "documentation lint src/index.mjs"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.0.1",
    "documentation": "^13.0.1",
    "rollup": "^2.18.0",
    "rollup-plugin-dev": "^1.1.2",
    "rollup-plugin-svelte": "git+https://github.com/ctavan/rollup-plugin-svelte.git#handle-missing-exports-gracefully",
    "semantic-release": "^17.0.8",
    "svelte": "^3.23.2",
    "testcafe": "^1.8.6"
  },
  "engines": {
    "node": ">=14.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/svelte-components.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/svelte-components/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/svelte-components#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-svelte-component",
      "arlac77/template-github"
    ]
  }
}
