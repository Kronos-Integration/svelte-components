{
  "name": "@kronos-integration/svelte-components",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "svelte": "./src/index.svelte",
  "description": "Svelte components for Kronos",
  "keywords": [
    "component",
    "svelte",
    "web"
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "start": "rollup -c example/rollup.config.mjs -w",
    "test": "testcafe $BROWSER:headless tests/*-test.js -s test-results --app-init-delay 1000 --app \"rollup -c example/rollup.config.mjs -w\""
  },
  "dependencies": {
    "@kronos-integration/endpoint": "^9.1.7",
    "@kronos-integration/service": "^9.2.5",
    "svelte-guard-history-router": "^2.24.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "documentation": "^13.0.2",
    "rollup": "^2.23.1",
    "rollup-plugin-dev": "^1.1.2",
    "rollup-plugin-svelte": "git+https://github.com/ctavan/rollup-plugin-svelte.git#handle-missing-exports-gracefully",
    "semantic-release": "^17.1.1",
    "svelte": "^3.24.1",
    "testcafe": "^1.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/svelte-components.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/svelte-components/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/svelte-components#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-svelte-component",
      "arlac77/template-github"
    ]
  }
}
